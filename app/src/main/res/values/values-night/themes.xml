<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
        <!-- Customize your dark theme here. -->
        <style name="Base.Theme.Kook" parent="Theme.Material3.DayNight.NoActionBar">
            <!-- Customize your light theme here. -->
            <item name="colorPrimary">#ED9A7F</item>
            <item name="colorPrimaryVariant">#638A79</item>
            <item name="colorOnPrimary">@color/white</item>
            <!-- Secondary brand color. -->
            <item name="colorSecondary">#FA9090</item>
            <item name="colorSecondaryVariant">#638979</item>
            <item name="colorOnSecondary">@color/black</item>
            <!-- Status bar color. -->
            <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
            <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
        </style>

        <style name="DialogAnimation">
            <item name="android:windowEnterAnimation">@anim/slide_in_bottom</item>
            <item name="android:windowExitAnimation">@anim/slide_out_bottom</item>
        </style>

        <style name="Theme.Kook" parent="Base.Theme.Kook" />
